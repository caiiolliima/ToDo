generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id        String   @id @default(cuid())
  title     String
  concluded Boolean  @default(false)
  state     String   @default("Created")
  userId    String
  createdAt DateTime @default(now())
}
